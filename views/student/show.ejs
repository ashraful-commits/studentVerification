<div class="container">
    <div class="row justify-content-center my-2">
        <div class="col-md-5">
            <div class="card">
                <div class="card-hearder">
                   <a class="btn btn-primary text-center w-100" href="/">Back</a>
                </div>
                <div class="card-body text-center">
                    <h5><%= students.name %> data showing</h5>
                    <img class="card-img-top" src="/images/<%= students.photo %> " alt="">
                    <h4><%= students.name %> </h4>
                    <hr>
                    <h5><%= students.email %> </h5>
                    <h5><%= students.cell %> </h5>
                   
                </div>
            </div>
        </div>
    </div>
</div>